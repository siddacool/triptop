{
  "name": "triptop",
  "version": "4.0.7",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test": "vitest",
    "svelte-check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check": "pnpm run svelte-check && eslint . && pnpm run lint",
    "check:fix": "pnpm run svelte-check && eslint . && pnpm run format"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-netlify": "^5.2.2",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@vitest/browser": "^3.2.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "playwright": "^1.53.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "sass": "^1.90.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.3",
    "vitest-browser-svelte": "^0.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.15.1",
  "volta": {
    "node": "22.18.0",
    "pnpm": "10.15.1"
  },
  "dependencies": {
    "@flightlesslabs/colors": "^0.2.0",
    "@flightlesslabs/grid": "^0.2.0",
    "@flightlesslabs/utils": "^0.3.0",
    "@iconify/svelte": "^5.0.1",
    "@internationalized/date": "^3.9.0",
    "bits-ui": "^2.9.4",
    "dayjs": "^1.11.13",
    "dexie": "^4.2.0",
    "nanoid": "^5.1.5"
  }
}
