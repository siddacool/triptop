{
  "name": "triptop",
  "private": true,
  "version": "3.7.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:host": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "svelte-check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "check": "npm run svelte-check && eslint . && npm run lint",
    "check:fix": "npm run svelte-check && eslint . && npm run format"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.32.0",
    "@sveltejs/adapter-netlify": "^5.1.0",
    "@sveltejs/kit": "^2.27.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "sass": "^1.89.2",
    "svelte": "^5.37.3",
    "svelte-check": "^4.3.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.0.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "@parcel/watcher"
    ]
  },
  "volta": {
    "node": "22.14.0",
    "pnpm": "10.5.2"
  },
  "packageManager": "pnpm@10.5.2",
  "dependencies": {
    "@iconify/svelte": "^5.0.1",
    "dayjs": "^1.11.13",
    "dexie": "^4.0.11",
    "nanoid": "^5.1.5"
  }
}
