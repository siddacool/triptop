{
  "name": "triptop",
  "private": true,
  "version": "3.5.8",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:host": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "svelte-check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "check": "npm run svelte-check && eslint . && npm run lint",
    "check:fix": "npm run svelte-check && eslint . && npm run format"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.22.0",
    "@sveltejs/adapter-netlify": "^4.4.2",
    "@sveltejs/kit": "^2.19.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-svelte": "^3.1.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "sass": "^1.85.1",
    "svelte": "^5.23.0",
    "svelte-check": "^4.1.5",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "@parcel/watcher"
    ]
  },
  "volta": {
    "node": "22.13.1",
    "pnpm": "10.5.2"
  },
  "packageManager": "pnpm@10.5.2",
  "dependencies": {
    "@iconify/svelte": "^4.2.0",
    "dayjs": "^1.11.13",
    "dexie": "^4.0.11",
    "nanoid": "^5.1.3"
  }
}
